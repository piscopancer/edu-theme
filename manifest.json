{
  "manifest_version": 2,
  "name": "EDU PGU Dark",
  "description": "ЕДУ ПГУ | Тёмная тема",
  "version": "1.0",
  "icons": {
    "128": "images/ICON.png"
  },
  "page_action": {
    "default_icon": {
      "128": "images/ICON.png"
    }
  },
  "content_scripts": [{
    "matches": ["https://edu.pgu.ru/*"],
    "run_at": "document_start",
    "js": ["/index.js"],
    "css": ["/styles/main.css"]
  }]
}